<%#= debug sandwich %>
<h3><%= sandwich.name %></h3>
<ul class="playlist">
  <% sandwich.posts.each do |post| %>
    <li>
      <a id="<%= "post-#{post.id}" %>" class="post" href="<%= post.mp3.url %>">
        <p><%= post.title %></p>
        <p><%= post.artist %></p>
      </a>
    </li>
  <% end %>
</ul>